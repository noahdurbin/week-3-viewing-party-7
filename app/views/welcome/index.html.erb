<% unless current_user %>
  <%= button_to "Create New User", register_path, method: :get %>
<% end %>

<% unless !current_user %>
  <section class='existing-users'>
      <p>Existing Users: </p>
      <ul>
      <% @users.each do |user| %>
          <li><%= user.email %></li>
      <% end %>
      </ul>
  </section>
<% end %>
